const  prompt  =  require ('prompt-sync')();

const  custoF  =  Number (prompt ( 'Qual é o custo de fábrica do carro?'));

const  porcDistr  =  custoF  *  0.28;
const impostos  = custoF  * 0.45 ;  
const  custoTotal  =  custoF  +  porcDistr  +  impostos;

console.log ( `O custo ao consumidor do carro é: R$ ${ custoTotal }`);